#include <bits/stdc++.h>

using namespace std;

int main()
{
    int num;
    cin >> num;
    string res;
    switch (num) {
        case 1:
            res = "one";
            break;
        case 2:
            res = "two";
            break;
        case 3:
            res = "three";
            break;
        case 4:
            res = "four";
            break;
        case 5:
            res = "five";
            break;
        case 6:
            res = "six";
            break;
        case 7:
            res = "seven";
            break;
        case 8:
            res = "eight";
            break;
        case 9:
            res = "nine";
            break;
        default:
            res = "Greater than 9";
    }
    cout << res;
    return 0;
}

