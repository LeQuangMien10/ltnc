#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    string s;
    cin >> n;
    for (int j = 0; j < n; j++)
    {
        cin >> s;
        int len = s.length();
        for (int i =0; i < len; i += 2)
            cout << s[i];
        cout << ' ';
        for (int i = 1; i < len; i += 2)
            cout << s[i];
        cout << endl;
    }
    return 0;
}
