#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    string s1, s2;
    getline(cin, s1);
    getline(cin, s2);

    cout << s1.length() << " " << s2.length() << endl;
    cout << s1 + s2 << endl;

    char temp = s1[0];
    s1[0] = s2[0];
    s2[0] = temp;

    cout << s1 << " " << s2 << endl;

    return 0;
}
